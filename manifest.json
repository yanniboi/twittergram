{
  "manifest_version": 2,
  "name": "TwitterGram",
  "description": "This extension shows instagram images on twitter.",
  "version": "1.0",
  "content_scripts": [{
    "js": [ "jquery.min.js", "content_script.js"  ],
    "matches": [ "http://twitter.com/*", "https://twitter.com/*" ]
  }],
  "permissions" : [
    "tabs",
    "http://*/*"
  ]
}